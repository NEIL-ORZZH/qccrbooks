{
  "version": "1.0.0",
  "bundles": [
    {
      "uri": "main",
      "pkg": "com.qccr.books.app.main"
    },
    {
      "uri": "topic",
      "pkg": "com.qccr.books.app.topic",
      "rules": {
        "fragment": ".TopicFragment"
      }
    },
    {
      "uri": "message",
      "pkg": "com.qccr.books.app.message",
      "rules": {
        "fragment": ".MessageFragment"
      }
    },
    {
      "uri": "user",
      "pkg": "com.qccr.books.app.user",
      "rules": {
        "fragment": ".UserFragment"
      }
    },
    {
      "uri": "lib.util",
      "pkg": "com.qccr.books.lib.util"
    }
  ]
}